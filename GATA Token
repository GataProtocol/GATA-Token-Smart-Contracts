pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract GATAToken is ERC20Burnable, Ownable {
    uint256 private constant TOTAL_SUPPLY = 10000000000 * 10**18; // 10 billion GATA Tokens with 18 decimals

    constructor() ERC20("GATA Token", "GATA") {
        _mint(msg.sender, TOTAL_SUPPLY);
    }
}
